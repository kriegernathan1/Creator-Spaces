{
   "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules/**": true,
    "**/dist/**": true
   }, 
   "editor.rulers": [
    {
      "column": 80,
    }
   ],
   "editor.formatOnSave": true,
   "source.organizeImports": "explicit"
}
